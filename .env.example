# CommCare Forge Environment Variables
# Copy this file to .env and fill in the values

# Required: Your Anthropic API key
ANTHROPIC_API_KEY=sk-ant-...

# Default HQ server (usually www.commcarehq.org)
HQ_SERVER=www.commcarehq.org

# Your CommCare HQ project space domain
HQ_DOMAIN=

# Max number of fix-and-retry attempts during validation
MAX_VALIDATION_RETRIES=5
